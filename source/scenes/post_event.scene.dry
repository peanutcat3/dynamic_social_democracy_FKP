        Q.rb_strength += 50;
    }
    if (Q.schleicher_time == 4 && (Q.unemployed >= 42 || (Q.workers_nsdap_normalized + Q.workers_kpd_normalized) >= 55 || Q.schleicher_spd_r >= 4)) {
        Q.schleicher_public_works_fast = 1;
