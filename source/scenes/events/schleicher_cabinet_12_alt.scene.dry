title: The "Reichstag Fire Decree" is signed by Hindenburg.
on-arrival: {!
Q.kpd_explode = 1;

Q.schleicher_spd_r -= 2;
Q.schleicher_spd_influence -= 1;
Q.schleicher_spd_support -= 3;
Q.schleicher_approval -= 1;

Q.pro_republic -= 10;

Q.rfb_militancy *= 0.8;
Q.rfb_strength *= 0.6;

Q.workers_kpd -= 8;
Q.workers_sapd -= 6;

Q.new_middle_kpd -= 3;
Q.new_middle_sapd -= 3;

Q.unemployed_kpd -= 6;
Q.unemployed_sapd -= 6;

Q.kpd_banned = 1;
Q.sapd_banned = 1;

if (Q.prussia_leader === "Goerdeler") {
  Q.rfb_militancy *= 0.9;
  Q.rfb_strength *= 0.8;
  Q.workers_kpd -= 2;
  Q.unemployed_kpd -= 2;
  Q.workers_sapd -= 2;
  Q.unemployed_sapd -= 2;
}

for (var party of Q.parties) {
    Q[party+'_r_pre'] = Q[party+'_r'];
}

Q.kpd_r = 0;
Q.sapd_r = 0;

total_r_sum = 0;

for (var party of Q.parties) {
    total_r_sum += Q[party + '_r'];
}

if (total_r_sum !== 100) {
    let normalization_factor = 100 / total_r_sum;
    for (var party of Q.parties) {
        Q[party + '_r'] = Math.round(Q[party + '_r'] * normalization_factor * 10) / 10;
    }
}
!}
