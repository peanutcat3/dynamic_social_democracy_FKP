Q.workers_dnef += 6;
Q.new_middle_dnef += 6;
Q.old_middle_dnef += 2;
Q.rural_dnef += 2;
Q.catholics_dnef += 2;
Q.unemployed_dnef += 6;

Q.dnef_relation -= 10;

if (Q.vice_chancellor == "Strasser" && Q.z_in_gov) Q.vice_chancellor = "Stegerwald";
if (Q.vice_chancellor == "Strasser" && !Q.z_in_gov) Q.vice_chancellor = 0;
if (Q.prussia_leader == "Strasser") Q.prussia_leader = "Goerdeler";

